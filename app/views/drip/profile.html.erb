              <a class="navbar-brand" to="/drip/">drip8</a>
            </div>
            <ul class="nav navbar-nav">
                <li class="dropdown">
                  <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">{{::profile.name.split(" ")[0]}} <span class="caret"></span></a>
                  <ul class="dropdown-menu">
                    <li class="dropdown-link" to="/drip/profile/" ng-click="passProfile( profile )"><a>Profiles</a></li>
                    <li role="separator" class="divider"></li>
                    <li class="dropdown-link" to="/logout"><a>Logout</a></li>
                  </ul>
